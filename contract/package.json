{
  "name": "@app/contract",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "npm run prisma:generate",
    "build": "nest build",
    "format": "prettier --check --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "predev": "npm run prisma:generate",
    "dev": "npm run start:dev",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "prestart:prod": "npx prisma migrate deploy",
    "start:prod": "node dist/src/main",
    "prestart:feat": "npx prisma migrate reset --force --skip-generate",
    "start:feat": "node dist/src/main",
    "pregql:generate-schema": "npm run prisma:generate",
    "gql:generate-schema": "ts-node src/jobs/generate-schema",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:report": "eslint \"{src,apps,libs,test}/**/*.ts\" -o eslint_report.json || echo 'errors, but eslint_report.json created'",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest",
    "prisma:generate": "prisma generate",
    "preprisma:generate-types": "rm -rf ./types/graphql/@generated && cat ./prisma/type-generators.partial.prisma ./prisma/schema.prisma > ./prisma/combined-schema.prisma",
    "prisma:generate-types": "npx prisma generate --schema=./prisma/combined-schema.prisma",
    "pretest:e2e": "npx dotenv-cli -y -e .env.test -- npx prisma migrate deploy",
    "test:e2e": "npx dotenv-cli -y -e .env.test -- jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles --forceExit",
    "ci:test:report": "npm run ci:test:unit -- --coverage || true # && npm run ci:test:e2e -- --coverage || true",
    "ci:test:unit": "jest --ci --testLocationInResults",
    "ci:test:e2e": "jest --ci --testLocationInResults --config ./test/jest-e2e.json --runInBand",
    "pregql:publish-schema": "npm run gql:generate-schema",
    "gql:publish-schema": "rover subgraph publish freelancenl-devsupergraph@develop --name core --schema ./schema.gql --routing-url=http://localhost:3007",
    "db:migrate": "npx prisma migrate deploy",
    "db:migrate:clean": "npx prisma migrate clean",
    "db:schema:sort": "npx prisma-schema-sorter sort --schema=\"./prisma/schema.prisma\"",
    "generate-encryption-key": "bash scripts/setup-local-encryption-key.sh"
  },
  "dependencies": {
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/schedule": "^4.0.2",
    "@ntegral/nestjs-sentry": "^4.0.1",
    "@package/authorization": "*",
    "@package/azure-service-bus": "*",
    "@package/config": "*",
    "@package/general": "*",
    "@package/pipeline": "*",
    "@package/prisma-select": "*",
    "@package/product": "*",
    "@package/service": "*",
    "@prisma/client": "5.11.0",
    "dayjs": "^1.11.10",
    "graphql-type-json": "^0.3.2",
    "nestjs-prisma": "^0.22.0",
    "p-map": "^4.0.0",
    "prisma-class-validator-generator": "^0.2.0",
    "prisma-nestjs-graphql": "20.0.2",
    "stripe": "^13.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@package/types": "*",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "prisma": "^5.11.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
